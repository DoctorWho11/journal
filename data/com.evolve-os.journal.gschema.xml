<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
	<schema id="com.evolve-os.journal" path="/com/evolve-os/journal/" gettext-domain="journal">
		<key name="window-state" type="b">
			<default>false</default>
			<summary>Current Window State</summary>
			<description>
				Current Window State (Maximized or not).
			</description>
		</key>
		<key name="scheme" type="s">
			<default>"classic"</default>
			<summary>Current Scheme</summary>
			<description>
				Current selected theme.
			</description>
		</key>
		<key name="window-width" type="i">
			<default>600</default>
			<summary>Saved width of the window.</summary>
			<description>
				The saved width of the window.
			</description>
		</key>
		<key name="window-height" type="i">
			<default>400</default>
			<summary>Saved height of the window.</summary>
			<description>
				The saved height of the window.
			</description>
		</key>
	</schema>
</schemalist>